@model ZiberTranslate.Web.ViewModels.ReviewViewModel
           
@{
    ViewBag.Title = "Index";
}

@section scripts {
<script src="@Url.Content("~/Scripts/translations.js")"></script>
}

<h2>Index</h2>

<ul>
    @foreach (var changedSet in Model.ChangedSets)
    {
        <li>
            <a href="@Url.Action("Set", new { id = changedSet.SetId, language = changedSet.Language})">@changedSet.SetName (@changedSet.Language)</a>
        </li>
    }
</ul>