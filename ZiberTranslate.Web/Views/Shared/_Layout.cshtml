<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jsdiff.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.7.2.min.js")" type="text/javascript"></script>
</head>

    <body>
        <header id="header">
            @RenderSection("header", false)
            @Html.ActionLink("Login", "Login", new { controller = "Security", returnUrl = this.Context.Request.Url.PathAndQuery })
            @Html.ActionLink("Logout", "Logout", "Security")
        </header>
        <section id="page">
            @RenderBody()
        </section>
        @RenderSection("scripts", false)
    </body>
</html>
