@model ZiberTranslate.Web.ViewModels.TranslateSetsViewModel

@{
    ViewBag.Title = "Engels";
}

<h2>Engelse vertalingen</h2>

<ul id="translate-sets">
    @foreach (var set in Model.Sets)
    {
        <li><a href="@Url.RouteUrl("Translations", new { setId = set.Id, language = "en" })">@set.Name</a></li>
    }
</ul>